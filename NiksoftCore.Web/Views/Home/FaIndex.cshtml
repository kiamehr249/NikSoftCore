<!doctype html>
<html lang="en">

<!-- Mirrored from tavonline.net/html/berlin/main/diagonal-slider.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 09 Dec 2020 15:52:13 GMT -->
<head>
    <title>مرکز تجارت بین الملل فکربرتر</title>
    <meta charset="UTF-8">
    <meta name="description" content="مرکز تجارت بین الملل فکربرتر">
    <meta name="keywords" content="تجارت, خدمات">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <link href="~/templates/UiTemplate/NikFace/rtl/images/fav.png" rel="shortcut icon" type="image/x-icon" />

    <link rel="stylesheet" href="~/templates/UiTemplate/NikFace/rtl/css/bootstrap.rtl.full.min.css" />
    <link rel="stylesheet" href="~/templates/UiTemplate/NikFace/rtl/css/reset.css" />
    <link rel="stylesheet" href="~/templates/UiTemplate/NikFace/rtl/css/style.css" />
    <link rel="stylesheet" href="~/templates/UiTemplate/NikFace/rtl/css/animate.css" />
    <link rel="stylesheet" href="~/templates/UiTemplate/NikFace/rtl/plugins/owlcarousel/assets/owl.carousel.css" />
    <link rel="stylesheet" href="~/templates/UiTemplate/NikFace/rtl/css/magnific-popup.css" />
    
    <link rel="stylesheet" href="~/templates/UiTemplate/NikFace/rtl/icon-fonts/font-awesome-4.5.0/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/templates/UiTemplate/NikFace/rtl/icon-fonts/essential-regular-fonts/essential-icons.css" />

</head>
<body class="diag">
    <script src="~/templates/UiTemplate/NikFace/rtl/js/jquery-2.1.4.min.js"></script>
    <script src="~/lib/tinymce/tinymce.min.js" referrerpolicy="origin"></script>
    <script src="~/lib/noty/noty.js" type="text/javascript"></script>

    <h1>@ViewBag.Error</h1>
    <div class="loader-wrapper">
        <img src="~/templates/UiTemplate/NikFace/rtl/images/logo-1-1.png" alt="">
        <div class="loader"></div>
    </div>

    <nav>
        <div class="row">
            @await Component.InvokeAsync("WMenus", new { key = "m_menu" })
        </div>
    </nav>

    <section class="home slider" id="home">
        @await Component.InvokeAsync("WContents", new { key = "m_bigslider" })
        <svg class="diagonal home-left" width="21%" height="170" viewBox="0 0 100 102" preserveAspectRatio="none">
            <path d="M0 100 L100 100 L0 10 Z"></path>
        </svg>
        <svg class="diagonal home-right" width="80%" height="170" viewBox="0 0 100 102" preserveAspectRatio="none">
            <path d="M0 100 L100 100 L100 10 Z"></path>
        </svg>
    </section>
    <section class="about dgray-bg" id="about">
        <div class="container">
            @await Component.InvokeAsync("WProfileSearch")
        </div>
        <div class="about type-1 padbot_120">
            <div class="container">
                @await Component.InvokeAsync("WContents", new { key = "m_part2" })

                @await Component.InvokeAsync("WContents", new { key = "m_part3" })
            </div>
            <svg class="diagonal-gray" width="100%" height="170" viewBox="0 0 100 102" preserveAspectRatio="none">
                <path d="M0 0 L70 100 L100 0 Z"></path>
            </svg>
        </div>
    </section>


    <section class="portfolio" id="portfolio">
        <div class="container">
            @await Component.InvokeAsync("WContents", new { key = "m_part4" })
        </div>
        <svg class="diagonal-white" width="100%" height="170" viewBox="0 0 100 102" preserveAspectRatio="none">
            <path d="M0 0 L30 100 L100 0 Z"></path>
        </svg>
    </section>

    <!-- BLOG -->
    <section class="blog" id="blog">
        <div class="container-fluid dgray-bg padbot_200">
            <div class="container">
                <div class="section-title dright top_120 bottom_45">
                    <h2>رویدادها</h2>
                </div>
                <!-- Blogs -->
                @await Component.InvokeAsync("WContents", new { key = "m_part5", size = 3 })
            </div>
            <svg class="diagonal-gray" width="100%" height="170" viewBox="0 0 100 102" preserveAspectRatio="none">
                <path d="M0 0 L70 100 L100 0 Z"></path>
            </svg>
        </div>
    </section>

    <!-- CONTACT -->
    <section class="contact" id="contact">
        <div class="container">
            <div class="section-title dright top_90">
                <h2 class="bottom_30">تماس با ما</h2>
            </div>
            <div class="col-md-3">
                <!-- Contact Info -->
                <ul class="contact-info row">
                    <li>
                        ونک، خیابان خدامی، خیابان آفتاب، نبش کوچه هشتم، پلاک 34، ساختمان فکر برتر
                    </li>
                    <li>
                        <br />info@fekrebartar.com
                    </li>
                    <li style="direction: ltr;">۰۲۱ ۴۲۰۳۷۷۰۰۰</li>
                </ul>
                <div class="social-icons top_60 row">
                    <a class="fb" href="#"><i class="fa fa-facebook" aria-hidden="true"></i></a>
                    <a class="tw" href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a>
                    <a class="ins" href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a>
                    <a class="bh" href="#"><i class="fa fa-behance" aria-hidden="true"></i></a>
                    <a class="dr" href="#"><i class="fa fa-dribbble" aria-hidden="true"></i></a>
                </div>
            </div>
            <div class="col-md-7 form top_30 bottom_90 col-md-offset-2">
                <div class="page-title sub">
                    <h5>با ما در ارتباط باشید</h5>
                </div>
                <form class="contact-form top_60" method="POST" action="/">
                    <div class="row">
                        <!--Name-->
                        <div class="col-md-6">
                            <input id="con_name" name="con_name" class="form-inp requie" type="text" placeholder="نام">
                        </div>
                        <!--Email-->
                        <div class="col-md-6">
                            <input id="con_email" name="con_email" class="form-inp requie" type="text" placeholder="نام خانوادگی">
                        </div>
                        <div class="col-md-12">
                            <!--Message-->
                            <textarea name="con_message" id="con_message" class="requie" placeholder="متن خود را بنویسید" rows="8"></textarea>
                            <button id="con_submit" class="sitebtn top_30 pull-left" type="submit">ارسال</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </section>
    <footer>
        <div class="container">
            <div class="social">
                <a href="#">facebook </a>
                <a href="#">twitter </a>
                <a href="https://www.instagram.com/fekrebartar__itc">instagram </a>
                <a href="#">google plus </a>
                <a href="#">behance </a>
                <a href="#">dribbble  </a>
            </div>
            <p>تمامی حقوق اثر متعلق به شرکت فکربرتر می باشد.</p>
        </div>
    </footer>

    <script src="~/templates/UiTemplate/NikFace/rtl/js/bootstrap.min.js"></script>
    <script src="~/templates/UiTemplate/NikFace/rtl/js/wow.min.js"></script>
    <script src="~/templates/UiTemplate/NikFace/rtl/js/isotope.pkgd.min.js"></script>
    <script src="~/templates/UiTemplate/NikFace/rtl/js/typed.js"></script>
    <script src="~/templates/UiTemplate/NikFace/rtl/js/jquery.magnific-popup.min.js"></script>
    <script src="~/templates/UiTemplate/NikFace/rtl/js/jquery.superslides.min.js"></script>
    <script src="~/templates/UiTemplate/NikFace/rtl/plugins/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/templates/UiTemplate/NikFace/rtl/js/main.js"></script>
    <script>

        //SUPER SLİDER
        $('#slides').superslides({
            animation: 'fade',
            play: 3000
        });
    </script>


</body>

</html>
