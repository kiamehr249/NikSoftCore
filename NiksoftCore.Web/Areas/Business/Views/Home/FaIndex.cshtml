<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="content-type" content="text/html;charset=UTF-8" />
    <title>بازرگانی مرکز تجارت بین الملل</title>
    <meta name="description" content="مرکز تجارت بین الملل" />
    <meta name="author" content="فکر برتر" />
    <link rel="shortcut icon" href="~/templates/UiTemplate/NikFace/rtl/images/fav.png" type="image/x-icon" />

    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />

    <link rel="stylesheet" type="text/css" href="~/templates/UiTemplate/FbBusiness/css/font-awesome.min.css" />
    <link rel="stylesheet" type="text/css" href="~/templates/UiTemplate/FbBusiness/css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/templates/UiTemplate/FbBusiness/css/bootstrap-rtl.min.css" />
    <link rel="stylesheet" type="text/css" href="~/templates/UiTemplate/FbBusiness/css/screen.css" />
    <link rel="stylesheet" type="text/css" href="~/templates/UiTemplate/FbBusiness/rs-plugin/css/settings.css">
    <link rel="stylesheet" type="text/css" href="~/templates/UiTemplate/FbBusiness/rs-plugin/css/layers.css">
    <link rel="stylesheet" type="text/css" href="~/templates/UiTemplate/FbBusiness/rs-plugin/css/navigation.css">
</head>
<body>
    <script type="text/javascript" src="~/js/persian-date.min.js"></script>
    <script type="text/javascript" src="~/lib/VueJs/vue.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/js/jquery.js"></script>
    <script type="text/javascript" src="~/js/VueUtilities.js"></script>
    <script type="text/javascript" src="~/lib/noty/noty.js"></script>
    <script type="text/javascript" src="~/lib/tinymce/tinymce.min.js"></script>
    <script type="text/javascript" src="~/js/BaseSystem.js"></script>
    <div id="page">
        <header class="header-v2">
            <div class="top-header">
                <div class="container-fluid">
                    <ul class="contacts">
                        <li><i class="fa fa-phone"></i>54982000 21 098</li>
                        <li><a href="mailto:info@fekrebartar.ir"><i class="fa fa-envelope"></i>info@fekrebartar.ir</a></li>
                    </ul>

                    <ul class="socials">
                        <li><a href="#"><i class="fa fa-facebook-square"></i></a></li>
                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                        <li><a href="#"><i class="fa fa-dribbble"></i></a></li>
                        <li><a href="#"><i class="fa fa-pinterest"></i></a></li>
                        <li><a href="#"><i class="fa fa-linkedin-square"></i></a></li>
                    </ul>
                </div>
            </div>

            <div class="bottom-header">
                <div class="container-fluid">
                    <span class="mobile-navigation-toggle hidden-md hidden-lg">
                        <span class="menui top-menu"></span>
                        <span class="menui mid-menu"></span>
                        <span class="menui bottom-menu"></span>
                    </span>

                    @await Component.InvokeAsync("WMenus", new { key = "b_menu" })

                    <ul class="actions">
                        @await Component.InvokeAsync("WBasket")
                        <li class="custom-select">
                            <span class="default"><i class="fa fa-globe"></i><span class="content">Fa</span></span>
                            <ul class="select-options dismiss">
                                <li>FA</li>
                                <li>EN</li>
                                <li>AR</li>
                            </ul>
                        </li>
                        <li class="search-trigger">
                            <i class="icon fa fa-search dismiss"></i>
                            <div class="search-box dismiss">
                                <form class="search-form dismiss" action="javascript:void(0)">
                                    <div class="input-line dismiss">
                                        <input type="text" class="search-input dismiss" placeholder="Type and Hit Enter">
                                    </div>
                                </form>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
        </header>

        <div class="content-wrapper">
            <section class="hero-section">
                <h2 class="hidden">اسلایدر اصلی</h2>
                <div class="container">
                    @await Component.InvokeAsync("WContents", new { key = "b_bigslider" })
                </div>
            </section>

            <section class="features-section">
                <div class="container">
                    @await Component.InvokeAsync("WCompanies")
                </div>
            </section>

            <section class="features-section v2">
                <div class="container-fluid">
                    @await Component.InvokeAsync("WContents", new { key = "b_part2" })
                </div>
            </section>

            <section class="clients-section">
                <div class="container-fluid">
                    <div class="row bg-alfa pos-relative">
                        <div class="col-md-6">
                            @await Component.InvokeAsync("WContents", new { key = "b_part3" })
                            <div class="row content">
                                <div class="col-md-10 col-md-offset-1">
                                    <div class="clients-row">
                                        @await Component.InvokeAsync("WCopmanyLogos")
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-6 no-padding pos-static">
                            <div class="row-bg">
                                <img src="~/templates/UiTemplate/FbBusiness/img/clients-section.jpg" alt="clients bg">
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <section class="team-members-section">
                <div class="container">
                    <div class="section-header white">
                        <h5 class="subtitle">مناطق اقتصادی فعال</h5>
                        <h2>فعال ترین مناطق اقتصادی</h2>
                        <span><i></i></span>
                        <p>برخی از فعال ترین مناطق اقتصادی در این مجموعه</p>
                    </div>

                    <div class="slick-carousel team-members" data-speed="600" data-arrows="false" data-dots="true" data-infinite="true" data-items-desktop="4">
                        @await Component.InvokeAsync("WIndustrialPark")
                    </div>
                </div>
            </section>

            <section class="services-section">
                <div class="container">
                    @await Component.InvokeAsync("WContents", new { key = "b_part4" })
                </div>
            </section>
        </div>

        <footer>
            <div class="top-footer">
                <div class="container">
                    <div class="row isotope">
                        <div class="col-md-3 col-sm-6 col-xs-12 isotope-item">
                            <div class="widget widget_contact">
                                <h4 class="widget-title">اطلاعات تماس</h4>
                                <div class="widget-body">
                                    <ul class="contact-info clean-list">
                                        <li>
                                            <span class="title">آدرس:</span>
                                            <p>ونک، خیابان خدامی، خیابان آفتاب، نبش کوچه</p>
                                            <p>هشتم، پلاک 34، ساختمان فکرتر</p>
                                        </li>
                                        <li>
                                            <span class="title">تلفن:</span>
                                            <p>54982000 21 098 - شرکت</p>
                                        </li>
                                        <li>
                                            <span class="title">ایمیل:</span>
                                            <p><a href="mailto:info@fekrebartar.ir">info@fekrebartar.ir</a></p>
                                        </li>
                                        <li>
                                            <span class="title">ما را دنبال کنید: </span>
                                            <ul class="socials clean-list">
                                                <li><a href="#"><i class="fa fa-facebook-square"></i></a></li>
                                                <li><a href="#"><i class="fa fa-pinterest-square"></i></a></li>
                                                <li><a href="#"><i class="fa fa-twitter-square"></i></a></li>
                                                <li><a href="#"><i class="fa fa-google-plus-square"></i></a></li>
                                                <li><a href="#"><i class="fa fa-linkedin-square"></i></a></li>
                                                <li><a href="#"><i class="fa fa-behance-square"></i></a></li>
                                                <li><a href="#"><i class="fa fa-instagram"></i></a></li>
                                            </ul>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-3 col-sm-6 col-xs-12 isotope-item">
                            <div class="widget widget_instagram">
                                <h4 class="widget-title">اینستاگرام</h4>
                                <div class="widget-body">
                                    <ul class="instagram-feed clean-list" data-instagram="6" data-instagram-username="paul_garrat"></ul>
                                </div>
                            </div>
                        </div>

                        <div class="col-md-3 col-sm-6 col-xs-12 isotope-item">
                            <!--<div class="widget widget_recent_posts">
                                <h4 class="widget-title">آخرین پست ها</h4>
                                <div class="widget-body">
                                </div>
                            </div>-->
                        </div>

                        <div class="col-md-3 col-sm-6 col-xs-12 isotope-item">
                            <div class="widget widget_subscribe">
                                <h4 class="widget-title">خبرنامه</h4>
                                <div class="widget-body">
                                    <p>ارسال آخرین تحولات و خدمات</p>
                                    <form class="subscribe-form" action="javascript:void(0)">
                                        <input class="input-line text-left" type="text" placeholder="info@email">
                                        <input class="submit-btn" type="submit" value="ثبت ایمیل">
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div class="bottom-footer">
                <div class="container">
                    <p class="alignleft align-left copyright">
                        تمامی حقوف اثر متعلق به شرکت فکربرتر می باشد.
                    </p>

                    <p class="alignright align-right copyright">
                        ساخته شده توسط <a href="http://nisksoftgroup.ir">www.nisksoftgroup.ir</a>
                    </p>
                </div>
            </div>
        </footer>
    </div>

    @*<script src="https://maps.googleapis.com/maps/api/js"></script>*@
    <script type="text/javascript" src="~/js/VueMain.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/js/modernizr.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/js/nouislider.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/rs-plugin/js/jquery.themepunch.tools.min.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>

    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/rs-plugin/js/extensions/revolution.extension.video.min.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/rs-plugin/js/extensions/revolution.extension.slideanims.min.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/rs-plugin/js/extensions/revolution.extension.actions.min.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/rs-plugin/js/extensions/revolution.extension.layeranimation.min.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/rs-plugin/js/extensions/revolution.extension.kenburn.min.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/rs-plugin/js/extensions/revolution.extension.navigation.min.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/rs-plugin/js/extensions/revolution.extension.migration.min.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/rs-plugin/js/extensions/revolution.extension.parallax.min.js"></script>

    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/js/imagesloaded.pkgd.min.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/js/jquery.magnific-popup.min.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/js/isotope.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/js/slick.js"></script>
    <script type="text/javascript" src="~/templates/UiTemplate/FbBusiness/js/options.js"></script>
</body>
</html>